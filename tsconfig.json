{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./app/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@localTypes/*": ["src/localTypes/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@services/*": ["src/services/*"],
      "@store/*": ["src/store/*"],
      "@models/*": ["src/models/*"],
    }
  },
  "extends": "expo/tsconfig.base"
}
